# sum of real for child and parent 
set title "Sum of children and parent real time" 
set xlabel "Number of processes"
set ylabel "Time [s] "
set terminal svg size 1024,576 enhanced background rgb 'white'
set output 'both_real.svg'
plot "../res/clone_both_real" with lines lw 2 title "Clone",	"../res/vclone_both_real" with lines lw 2 title "Vclone",	"../res/vfork_both_real" with lines lw 2 title "Vfork",	"../res/fork_both_real" with lines lw 2 title "Fork"
unset output


# Sum of sum usr + sys time for parent and child
set title "Sum of parent and children User and system Time " 
set xlabel "Number of processes"
set ylabel "Time [s] "
set terminal svg size 1024,576 enhanced background rgb 'white'
set output 'both_sum.svg'
plot "../res/clone_both_sum" with lines lw 2 title "Clone",	"../res/vclone_both_sum" with lines lw 2 title "Vclone",	"../res/vfork_both_sum" with lines lw 2 title "Vfork",	"../res/fork_both_sum" with lines lw 2 title "Fork"
unset output


